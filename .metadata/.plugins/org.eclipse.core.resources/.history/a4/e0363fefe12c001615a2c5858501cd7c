public class Conta {
	
	private int agencia;
	private int numero;
	protected double saldo;

	void deposita(double valor){
		this.saldo = valor;
	}

	public void saca(double valor) {
		if(valor > this.saldo){
			System.out.println("Saldo Insuficiente");
		}else {
			this.saldo -= valor;
		}
	}
	
	public void atualiza(double taxa) {
		this.saldo += this.saldo * taxa;
	}
	
	public double getSaldo() {
		return this.saldo;
	}
}
